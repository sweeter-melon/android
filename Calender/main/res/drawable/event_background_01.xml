<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:width="50dp"
        android:height="50dp"
        android:left="@dimen/px002"
        android:top="@dimen/px003">

        <shape
            android:shape="rectangle">
            <solid android:color="@color/md_grey_400"/>

            <corners android:radius="5dp"/>
        </shape>

    </item>

    <item android:right="@dimen/px002"
        android:bottom="@dimen/px003">

        <shape android:shape="rectangle">
            <solid android:color="@color/event_color_01"/>
            <corners android:radius="5dp"/>
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp"/>
        </shape>

    </item>

</layer-list>